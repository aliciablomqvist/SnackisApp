@page
@model TestCategoriesModel

<h2>Categories</h2>
<ul>
    @foreach (var category in Model.Categories)
    {
        <li>
            <strong>@category.Name</strong>
            <ul>
                @foreach (var subCategory in category.SubCategory)
                {
                    <li>@subCategory.Name</li>
                }
            </ul>
        </li>
    }
</ul>
